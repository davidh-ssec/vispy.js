  "author": "Vispy Development Team",
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "bugs": {
    "url": "https://github.com/vispy/vispy.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vispy/vispy.git"
  },
  "homepage": "https://github.com/vispy/vispy",
  "keywords": [
    "jupyter",
